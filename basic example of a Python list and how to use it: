# Creating a list
fruits = ["appleğŸ", "bananağŸŒ", "cherryğŸ’", "pineappleğŸ"]

# Accessing elements
print(fruits[0])  #Output: appleğŸ
print(fruits[-1]) # Output: pineappleğŸ 

# Adding an element
fruits.append("watermelonğŸ‰")
print(fruits)  # ['appleğŸ', 'bananağŸŒ', 'cherryğŸ’', 'pineappleğŸ', 'watermelonğŸ‰']

# Removing an element
fruits.remove("bananağŸŒ")
print(fruits)  # ['appleğŸ', 'cherryğŸ’', 'pineappleğŸ', 'watermelonğŸ‰']

# Looping through a list
for fruit in fruits:
    print(fruit)

# List length
print("Total fruits:", len(fruits))
